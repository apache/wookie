<ivysettings>
  <settings defaultResolver="wookie-chain" />
  <resolvers>
    <chain name="wookie-chain">
      <filesystem name="main">
        <artifact pattern="${basedir}/${ivy.repository.dir}/lib/[artifact]-[revision].[type]" />
      </filesystem>>
      <ibiblio name="public" m2compatible="true"/>
    </chain>
  </resolvers>
</ivysettings>
